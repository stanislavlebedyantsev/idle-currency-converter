    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-range-requests": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-streams": "^5.1.3"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "start": "react-app-rewired start",
    "electron-dev": "concurrently \"SET BROWSER=none&&yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "dev": "concurrently \"SET BROWSER=none&&yarn start\"",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "e2e": "react-app-rewired test src/e2e",
    "eject": "react-scripts eject",
    "deploy": "firebase deploy",
    "login": "firebase login",
    "analyze": "source-map-explorer build/static/js/*",
    "init": "firebase init",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.example.currency-converter",
    "files": [
      "**/*",
      "*",
      "**",
      "/build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/src/$1"
    }
  },
  "description": "idle currency converter project",
  "main": "public/electron.js",
  "repository": "https://github.com/stanislavlebedyantsev/currency-converter-with-PWA.git",
  "author": "stanislavlebedyantsev <stanislav.lebedyantsev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-leaflet": "^2.8.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.12",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^6.0.0",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "jest-image-snapshot": "^4.4.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^8.0.0",
    "redux-mock-store": "^1.5.4",
    "source-map-explorer": "^2.5.2",
    "ts-jest": "^26.5.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "wait-on": "^5.2.1"
  },
  "homepage": "./"
}
